    <record id="more_button_generar_plame_total" model="ir.values">
        <field eval="'client_action_multi'" name="key2"/>
        <field eval="'hr.payslip.run'" name="model"/>
        <field name="name">Exportar Plame Sumarizado</field>
        <field eval="'ir.actions.server,%d'%planilla_generar_plame_total" name="value"/>
    </record>

    <!--Plame horas-->
